<filter log.*.*>
  @type geoip
</filter>


<match log.*.*>
  @type secure_forward
  shared_key password1!
  secure no
  self_hostname localhost.fluentd.col
    <server>
	  host localhost
	  port 9999
	</server>
  buffer_type memory
  buffer_chunk_limit 16m
  buffer_queue_limit 128
  flush_interval 30s
  flush_at_shutdown false
  retry_limit 17
  retry_wait 1s
  max_retry_wait 1m
  disable_retry_limit true
  num_threads 4
</match>

